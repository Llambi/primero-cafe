{
  "extends": "astro/tsconfigs/base",
  "plugins": [
    {
      "name": "@astrojs/ts-plugin"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": false,
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "moduleResolution": "node",
  "resolveJsonModule": true,
  "isolatedModules": true,
  "types": [
    "astro/client"
  ]
}